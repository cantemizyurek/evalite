{
  "name": "evalite-tests",
  "version": "0.0.11",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wireit",
    "test": "wireit",
    "lint": "wireit"
  },
  "wireit": {
    "dev": {
      "command": "vitest -w",
      "service": true,
      "dependencies": [
        "../../apps/evalite-ui:after-build",
        "../../packages/evalite:dev"
      ],
      "clean": false
    },
    "test": {
      "command": "vitest run",
      "dependencies": [
        "../../apps/evalite-ui:after-build",
        "../../packages/evalite:build"
      ]
    },
    "lint": {
      "command": "tsc"
    }
  },
  "dependencies": {
    "evalite": "workspace:*",
    "evalite-ui": "workspace:*",
    "strip-ansi": "^7.1.2",
    "autoevals": "^0.0.131",
    "@ai-sdk/openai": "^2.0.42",
    "ai": "^5.0.59"
  }
}
